---
import "../styles/global.scss";
import Links from "../Data/Links.astro";

const { pageName, title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="/favicon.svg" type="image/x-icon" />
    <title>{title}</title>
  </head>
  <body class={pageName}>
    <header>What's up</header>
    <Links />
    <slot />
  </body>
</html>
